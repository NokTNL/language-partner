  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Program",
      "program": "${workspaceFolder}/app.ts",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "type": "node"
    },
    {
      "type": "node-terminal",
      "name": "Run Script: start:api (language-partner)",
      "request": "launch",
      "command": "npm run start:api",
      "cwd": "${workspaceFolder}"
    }
  ]
}
